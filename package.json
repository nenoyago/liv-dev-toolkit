{
  "name": "neno-packages-workspace",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "ci": "pnpm install --frozen-lockfile",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "pnpm run build:design-system && concurrently -k -n design-system,demo -c yellow,cyan \"pnpm run build:design-system:watch\" \"pnpm run dev:wait\"",
    "predev": "pnpm run check-env",
    "prebuild:design-system": "pnpm run check-env",
    "prebuild:design-system:watch": "pnpm run check-env",
    "prestart:demo": "pnpm run check-env",
    "dev:wait": "wait-on dist/design-system/package.json && pnpm run start:demo",
    "build:design-system": "ng build design-system",
    "build:design-system:watch": "ng build design-system --watch",
    "start:demo": "ng serve demo",
    "storybook": "ng run design-system:storybook",
    "build-storybook": "ng run design-system:build-storybook",
    "prepare": "husky",
    "lint": "pnpm lint",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write \"**/*.{ts,html}\"",
    "check-env": "node ./scripts/check-env.js",
    "changeset:empty": "HUSKY=0 pnpm changeset --empty",
    "changeset:bump": "HUSKY=0 pnpm changeset"
  },
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": "22.14",
    "pnpm": "10.6.x",
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/forms": "^20.1.6",
    "@angular/platform-browser": "^20.1.6",
    "@angular/router": "^20.1.6",
    "@tailwindcss/postcss": "^4.1.11",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.2",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.5",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "^20.1.5",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.6",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/addon-styling-webpack": "^2.0.0",
    "@storybook/angular": "^9.1.2",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "commitizen": "^4.3.0",
    "concurrently": "^9.2.0",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.5",
    "ng-packagr": "^20.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "storybook": "^9.1.2",
    "style-loader": "^4.0.0",
    "typescript": "~5.8.2",
    "wait-on": "^8.0.4",
    "zone.js": "^0.15.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "(*.ts|*.html|*.css|*.scss)": "prettier --write",
    "*.ts": "eslint --fix"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@compodoc/compodoc",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "lmdb",
      "msgpackr-extract"
    ]
  }
}